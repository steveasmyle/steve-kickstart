{
  "name": "steve-kickstart",
  "version": "1.0.0",
  "description": "Kickstart project template",
  "author": "Steve Arnott",
  "license": "MIT",
  "browserslist": [
    "defaults and supports es6 and not ie 11"
  ],
  "scripts": {
    "build": "next build",
    "dev": "set PORT=3001&&next dev",
    "devmac": "PORT=3001 next dev",
    "eslint": "eslint .",
    "export": "next export -o _static",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "start": "next start",
    "stylelint": "stylelint --formatter=verbose **/*.ts*",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steveasmyle/steve-kickstart.git"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "axios": "^1.1.3",
    "framer-motion": "^7.2.1",
    "moment": "^2.29.4",
    "next": "^12.2.2",
    "polished": "^4.2.2",
    "postcss-syntax": "^0.36.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-moment": "^1.1.2",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "styled-components": "^5",
    "stylelint-config-recommended": "^9.0.0",
    "upgrade": "^1.1.0"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.17",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^7.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "jest-next-dynamic": "^1.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "stylelint": "^14.12.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^28.0.0",
